# Firebase Android
ANDROID_API_KEY=your_android_api_key
ANDROID_APP_ID=your_android_app_id
ANDROID_SENDER_ID=your_android_sender_id
ANDROID_PROJECT_ID=your_project_id
ANDROID_BUCKET=your_storage_bucket

# Firebase iOS
IOS_API_KEY=your_ios_api_key
IOS_APP_ID=your_ios_app_id
IOS_SENDER_ID=your_ios_sender_id
IOS_PROJECT_ID=your_project_id
IOS_BUCKET=your_storage_bucket
IOS_BUNDLE_ID=your_ios_bundle_id

# App Configuration
APP_NAME=Zarity
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true 